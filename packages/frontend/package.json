  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "msw:prepare": "node scripts/msw-prepare.mjs",
    "openapi:update": "orval --config orval.config.ts",
    "dev": "node scripts/msw-prepare.mjs && vite",
