    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.20",
    "jsdom": "^26.1.0",
    "msw": "^2.11.2",
    "orval": "^7.11.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
